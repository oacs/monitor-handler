  "devDependencies": {
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.15",
    "@types/on-finished": "^2.3.1",
    "@types/on-headers": "^1.0.0",
    "mongoose": "^5.4.11",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1"
  }
}